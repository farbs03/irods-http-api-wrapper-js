{
    "compilerOptions": {
        "target": "ES2020",
        "module": "Node16",
        "outDir": "./dist",
        "rootDir": "./",
        "forceConsistentCasingInFileNames": true,
        "verbatimModuleSyntax": true,
        "strict": true,
        "declaration": true,
        "sourceMap": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "allowJs": true,
    },
}
